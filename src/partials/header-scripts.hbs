<script>
var disappear = function(){
  return $(document).height() - $(window).height();
};

$(function(){
  $(window).scroll(function(){
  	if($(this).scrollTop() >= disappear()){
			$('.is-this-helpful-box').fadeOut();
		}
		else{
			$('.is-this-helpful-box').fadeIn();
		}
	});
});
</script>
